<html metal:extend-macro="login_template" i18n:domain="substanced">
 <head>
    <metal:slot metal:define-slot="head-more" />
 </head>
 <body>

  <metal:slot metal:fill-slot="before-login-form">
   <metal:slot metal:define-slot="before-login-form">

     <div class="alert alert-danger" i18n:translate="">
       Sorry, your account is not permitted to perform the requested action.
       You can try to log in using a different account below or you can
       <a onclick="javascript:history.go(-2)">return to where you were</a>.
     </div>

   </metal:slot>
  </metal:slot>

  <metal:slot metal:fill-slot="login-form-title">
   <metal:slot metal:define-slot="login-form-title">
    <h2 i18n:translate="">Log In as Another User</h2>
   </metal:slot>
  </metal:slot>

  <metal:slot metal:define-slot="password-reset-link">
  </metal:slot>

 </body>
</html>
